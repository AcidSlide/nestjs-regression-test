{
  "name": "test-api",
  "version": "2.0.1",
  "description": "Test API",
  "author": "Octal Philippines Inc.",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.10.14",
    "@nestjs/core": "^6.10.14",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.1",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/platform-fastify": "^6.10.14",
    "@nestjs/swagger": "^4.1.15",
    "@nestjs/typeorm": "^6.2.0",
    "bcrypt": "^3.0.7",
    "cache-manager": "^2.10.1",
    "cache-manager-redis-store": "^1.5.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.14.0",
    "dotenv": "^8.2.0",
    "dotenv-extended": "^2.7.1",
    "fastify-multipart": "^1.0.4",
    "fastify-swagger": "^2.5.0",
    "mysql": "^2.18.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pump": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "sharp": "^0.24.0",
    "typeorm": "^0.2.22",
    "uuid": "^3.4.0",
    "webpack": "^4.41.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.0",
    "@nestjs/schematics": "^6.8.2",
    "@nestjs/testing": "^6.10.14",
    "@types/bcrypt": "^3.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/fastify-multipart": "^0.7.0",
    "@types/hapi__joi": "^16.0.7",
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.8",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/pump": "^1.1.0",
    "@types/sharp": "^0.24.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
